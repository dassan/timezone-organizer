{
  "manifest_version": 3,
  "name": "Time Zone Organizer",
  "version": "1.0",
  "description": "Displays multiple time zones on your new tab page",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}